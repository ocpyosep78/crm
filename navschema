REGULAR
	USER
		FRAME + NAVBAR + MENU + CONTENT											DONE without CONTENT for now
	GUEST
		FRAME + NAVBAR + MENU + LOGIN											DONE


AJAX
	USER
		[NAVBAR +] [MENU +] CONTENT
	GUEST
		IF !LOGIN THEN ERROR
		IF LOGIN AND !CORRECT THEN ERROR
		IF LOGIN AND CORRECT THEN RELOAD
		
		
		
IF REGULAR THEN PAGE::PAGE()

ELSEIF USER THEN XAJAX_PROCESSREQUESTS()

ELSEIF !ISXAJAX(LOGIN) THEN ERROR

ELSE IF ATTEMPTLOGIN THEN RELOAD
